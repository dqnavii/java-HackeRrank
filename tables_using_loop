import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;



public class Solution 
{
    public static void main(String[] args) 
    {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int ans;
int i;
for(i=1;i<=10;i=i+1)
   {
     ans=n*i;
     System.out.println(n+" x "+i+" = "+ans);
   }
          
    }
}
